  <script>
    const contentSkipBtn = document.querySelector('.content-skip');
    contentSkipBtn.addEventListener('click', function () {
      contentSkipBtn.style.display = 'none'
    });
    function scrollPos() {
      return document.body.scrollTop || window.scrollY
    }
    window.addEventListener('load', () => {
      if (document.querySelector('.header-text-container p').getBoundingClientRect().y < window.innerHeight) contentSkipBtn.style.display = 'none'
      document.documentElement.style.scrollBehavior = 'smooth'
    });
    window.addEventListener('scroll', () => {
      if (scrollPos() !== 0) contentSkipBtn.style.display = 'none'
    }, { passive: true })
    if (scrollPos() === 0) contentSkipBtn.style.display = 'inline'

    document.querySelector('.category-box').addEventListener('mouseover', e => {
      if (!e.target.classList.contains('category-icon')) return
      e.target.parentNode.previousElementSibling.checked = true
    })
  </script>
